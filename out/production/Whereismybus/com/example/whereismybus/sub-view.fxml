<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.whereismybus.SubViewController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="내 버스는 어디에..">
         <font>
            <Font name="HYPost-Light" size="50.0" />
         </font>
      </Label>
      <Button fx:id="goMainBtn" layoutX="1072.0" layoutY="17.0" mnemonicParsing="false" onAction="#goMainScene" prefHeight="40.0" styleClass="btn-home" text="홈으로">
         <font>
            <Font name="HYPost-Light" size="25.0" />
         </font>
      </Button>
      <TextField fx:id="stationId" layoutX="521.0" layoutY="19.0" prefHeight="40.0" prefWidth="400.0" promptText="검색어를 입력하세요." styleClass="textfield">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Text fx:id="result" layoutX="151.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="result">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Button fx:id="resultBtn" layoutX="930.0" layoutY="17.0" mnemonicParsing="false" onAction="#search" prefHeight="40.0" text="결과보기">
         <font>
            <Font name="HYPost-Light" size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
